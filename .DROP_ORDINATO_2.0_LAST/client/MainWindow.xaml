        <Button Content="GET UPD" HorizontalAlignment="Left" Margin="38,162,0,0" VerticalAlignment="Top" Width="75" Click="Get_Click"/>
        <Button Content="QUIT" HorizontalAlignment="Left" Margin="38,203,0,0" VerticalAlignment="Top" Width="75" Click="Quit_Click"/>
